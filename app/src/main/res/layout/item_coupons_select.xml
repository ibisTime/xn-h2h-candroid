<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/lin_no_select"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"

        >

        <View style="@style/line_gray" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="15dp"
            android:paddingLeft="@dimen/layout_left_right_interval"
            android:paddingRight="@dimen/layout_left_right_interval"
            android:paddingTop="15dp">

            <TextView
                android:id="@+id/tv_coupon_name"
                style="@style/text_black"
                android:layout_marginRight="30dp"
                android:textSize="14sp"
                tools:text="剑0.00元" />

            <CheckBox
                android:id="@+id/checkbox_coupon_select"
                style="@style/check_pay"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_gravity="right"
                android:clickable="false" />

        </FrameLayout>


    </LinearLayout>
</layout>
